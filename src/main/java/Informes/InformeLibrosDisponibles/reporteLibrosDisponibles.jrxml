	<queryString language="SQL">
		<![CDATA[SELECT 
    l.titulo,
    l.autor,
    l.genero,
    e.estado,
    s.nombre AS sucursal,
    s.direccion AS direccion_sucursal
FROM 
    Libro l
JOIN 
    Ejemplar e ON l.id = e.id_libro
JOIN 
    Sucursal s ON e.id_sucursal = s.id
WHERE 
    e.estado = 'disponible';]]>
	</queryString>
